@charset "utf-8";
/* CSS Document */

body, html {
    width: 100%;
}
*{
    margin:0px;
    padding:0px;
	
}
.red{
	color:#FF0000;
}
body{
    font:12px tahoma;
    text-align: center;
}
.clear{
    clear:both;
}
ul{
    list-style-type:none
}
#wrapper{
    width:1000px;
	height:1850px;
    margin:0px auto;

}
