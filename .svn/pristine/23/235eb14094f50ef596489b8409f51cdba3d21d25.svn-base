#categories{
    width:100px;
    height:300px;
    background:#FFF;
    float:left;	
}
#categoriescontent li{
    list-style: none;
    position: relative;
	color:#FFF;
}
#categoriescontent li a{
    text-decoration: none;
	color:#000;
    display: block;
    font-weight: bold;
    display: block;
    background: #FFF;
    border-bottom: 1px solid #000;
    width: 100px;
    height: 37px;
    text-indent: 30px;
    line-height: 45px;
    text-transform: uppercase;
}
#categoriescontent li a:hover{
	background:#000;
	color:#FFF;
}
